# DAQ (Data Acquisition System)

protocol = udp
port = 8080
name = daq
endianness = big
triggers = triggers
multicast = 224.0.0.1


## measurements ##

# physical telemetry (off the network)
MILLISEC    4 int unsigned
MICROSEC    4 int unsigned
SEQ_NUM     4 int unsigned
ADC0_STATUS 3 int unsigned
IEPE0_DATA  3 int signed
IEPE1_DATA  3 int signed
IEPE2_DATA  3 int signed
IEPE3_DATA  3 int signed
ADC1_STATUS 3 int unsigned
FB0_DATA    3 int signed
FB1_DATA    3 int signed
CL0_DATA    3 int signed
CL1_DATA    3 int signed
TC0_DATA    4 int unsigned
TC1_DATA    4 int unsigned
TC2_DATA    4 int unsigned
TC3_DATA    4 int unsigned

# virtual measurements (calculated from physical telemetry)
IEPE0_VOLTS 8 float
IEPE1_VOLTS 8 float
IEPE2_VOLTS 8 float
IEPE3_VOLTS 8 float
FB0_VOLTS   8 float
FB1_VOLTS   8 float
CL0_VOLTS   8 float
CL1_VOLTS   8 float
TC0_REMOTE  8 float
TC1_REMOTE  8 float
TC2_REMOTE  8 float
TC3_REMOTE  8 float
TC0_AMBIENT 8 float
TC1_AMBIENT 8 float
TC2_AMBIENT 8 float
TC3_AMBIENT 8 float
TC0_STATUS  1 int unsigned
TC1_STATUS  1 int unsigned
TC2_STATUS  1 int unsigned
TC3_STATUS  1 int unsigned
TC0_CORRECTED 8 float
TC1_CORRECTED 8 float
TC2_CORRECTED 8 float
TC3_CORRECTED 8 float
PT0_PSI       8 float
PT1_PSI       8 float
LC0_FORCE     8 float
PT0_PSI_MEAN  8 float
PT1_PSI_MEAN  8 float
LC0_FORCE_MEAN  8 float


## telemetry packets ##

# ADC0
8080 {
MILLISEC
MICROSEC
SEQ_NUM
ADC0_STATUS
IEPE0_DATA
IEPE1_DATA
IEPE2_DATA
IEPE3_DATA
}

# ADC1
8081 {
MILLISEC
MICROSEC
SEQ_NUM
ADC1_STATUS
FB0_DATA
FB1_DATA
CL0_DATA
CL1_DATA
}

# thermocouples
8082 {
MILLISEC
MICROSEC
SEQ_NUM
TC0_DATA
TC1_DATA
TC2_DATA
TC3_DATA
}

# calcualted ADC0 voltages
virtual {
IEPE0_VOLTS
IEPE1_VOLTS
IEPE2_VOLTS
IEPE3_VOLTS
}

# calculated ADC1 voltages
virtual {
FB0_VOLTS
FB1_VOLTS
CL0_VOLTS
CL1_VOLTS
}

# calculated thermocouple temperatures
virtual {
TC0_STATUS
}

virtual {
TC0_REMOTE
TC0_AMBIENT
TC0_CORRECTED
}

virtual {
TC1_STATUS
}

virtual {
TC1_REMOTE
TC1_AMBIENT
TC1_CORRECTED
}

virtual {
TC2_STATUS
}

virtual {
TC2_REMOTE
TC2_AMBIENT
TC2_CORRECTED
}

virtual {
TC3_STATUS
}

virtual {
TC3_REMOTE
TC3_AMBIENT
TC3_CORRECTED
}

# calculated load cell values
virtual {
LC0_FORCE
LC0_FORCE_MEAN
}

# calculated pressure transducer values
virtual {
PT0_PSI
PT1_PSI
PT0_PSI_MEAN
PT1_PSI_MEAN
}
