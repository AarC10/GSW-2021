protocol = udp
port = 8080

name = aprs_forwarder

# [measurement name] [total measurement size in bytes] [optional type of int, float, or string, default is int] [optional signed or unsigned, default is signed]
# signed/unsigned cannot be specified without a type

# use same endianness as system

APRS_LAT  4 float signed
APRS_LONG 4 float signed

# telemetry packets
8081 {
APRS_LAT
APRS_LONG
}
