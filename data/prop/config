# Engine Controller

protocol = udp
port = 8080
name = engine_controller_sim
endianness = little
multicast = 224.0.0.1


## measurements ##

# relays
SOLENOID1 2 int unsigned
SOLENOID2 2 int unsigned
SOLENOID3 2 int unsigned
IGNITER1  2 int unsigned

# dummy light tower
LED_G     2 int unsigned
LED_Y     2 int unsigned
LED_R     2 int unsigned
LED_BUZZ  2 int unsigned

# command sequence number
SEQ_NUM 4 int unsigned

# heartbeat number
HEARTBEAT 2 int unsigned

# mode number
MODE 2 int unsigned

# safing number
SAFING 2 int unsigned

## telemetry packets ##

# sequence number only
8080 {
SEQ_NUM
}

# solenoids
8081 {
SOLENOID1
SOLENOID2
SOLENOID3
SEQ_NUM
}

# igniters
8082 {
IGNITER1
SEQ_NUM
}

# light tower
8083 {
LED_G
LED_Y
LED_R
LED_BUZZ
SEQ_NUM
}

# everything
8084 {
SOLENOID1
SOLENOID2
SOLENOID3
IGNITER1
LED_G
LED_Y
LED_R
LED_BUZZ
SEQ_NUM
HEARTBEAT
}

8085 {
SEQ_NUM
MODE
SAFING
}
