# Engine Controller Simulation

protocol = udp
port = 8080

name = engine_controller_sim

endianness = little


## measurements ##

# relays
SOLENOID1 1 int unsigned
SOLENOID2 1 int unsigned
SOLENOID3 1 int unsigned
SOLENOID4 1 int unsigned
IGNITER1  1 int unsigned

# dummy light tower
LED_G     1 int unsigned
LED_Y     1 int unsigned
LED_R     1 int unsigned
LED_BUZZ  1 int unsigned

# command sequence number
SEQ_NUM 4 int unsigned


## telemetry packets ##

# solenoids
8081 {
SOLENOID1
SOLENOID2
SOLENOID3
SOLENOID4
}

# igniter(s)
8082 {
IGNITER1
}

# light tower
8083 {
LED_G
LED_Y
LED_R
LED_BUZZ
}

# sequence number only
8084 {
SEQ_NUM
}

